{
  "version": 2,
  "builds": [
    {
      "src": "api/get-paystack-secret-key.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/get-paystack-secret-key",
      "dest": "/api/get-paystack-secret-key.js"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/referral",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
