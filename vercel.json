{
  "rewrites": [
    {
      "source": "/server.js",
      "destination": "/api/server.js"
    },
    {
      "source": "/referral",
      "destination": "/"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "PAYSTACK_SECRET_KEY": "@paystack-secret-key" 
  }
}
